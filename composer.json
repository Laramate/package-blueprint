{
    "name": "laramate/package-blueprint",
    "description": "Laramate package blueprint for laravel packages",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Laramate",
            "email": "coding@laramate.de",
            "homepage": "https://www.laramate.de/",
            "role": "Team"
        },
        {
            "name": "Tobi Kivelip",
            "email": "tkivelip@gmail.com",
            "homepage": "https://www.laramate.de/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Laramate\\PackageBlueprint\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laramate\\PackageBlueprint\\Tests\\": "tests/"
        }
    }
}
